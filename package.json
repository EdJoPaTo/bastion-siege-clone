{
	"name": "bastion-siege-clone",
	"private": true,
	"version": "1.4.6",
	"license": "MIT",
	"repository": "EdJoPaTo/bastion-siege-clone",
	"author": {
		"name": "Edgar Toll",
		"email": "EdJoPaTo@gmail.com",
		"url": "EdJoPaTo.de"
	},
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"build": "del dist && tsc",
		"prepack": "npm run build",
		"start": "ts-node source/index.ts",
		"pretest": "tsc",
		"test": "xo"
	},
	"config": {
		"tokenpath": "/run/secrets/bastion-siege-clone-token.txt"
	},
	"main": "dist",
	"types": "dist",
	"files": [
		"dist",
		"!*.test.*"
	],
	"dependencies": {
		"@sindresorhus/tsconfig": "^0.4.0",
		"array-filter-unique": "^2.1.0",
		"bastion-siege-logic": "^1.3.1",
		"random-item": "^3.0.0",
		"telegraf": "^3.28.0",
		"telegraf-i18n": "^6.5.0",
		"telegraf-inline-menu": "^3.5.2",
		"telegraf-session-local": "0.0.7",
		"ts-node": "^8.0.3",
		"typescript": "^3.4.3",
		"wikidata-entity-reader": "^1.0.0",
		"wikidata-entity-store": "^1.1.0",
		"wikidata-sdk": "^6.3.0",
		"wikidata-sdk-got": "^1.1.0"
	},
	"devDependencies": {
		"@types/node": "^12.0.2",
		"@typescript-eslint/eslint-plugin": "^1.10.2",
		"@typescript-eslint/parser": "^1.10.2",
		"del-cli": "^2.0.0",
		"eslint-config-xo-typescript": "^0.14.0",
		"xo": "^0.24.0"
	},
	"xo": {
		"extends": "xo-typescript",
		"semicolon": false,
		"extensions": [
			"ts"
		],
		"rules": {
			"import/named": "warn",
			"@typescript-eslint/promise-function-async": "off",
			"@typescript-eslint/array-type": "off",
			"@typescript-eslint/semi": [
				"error",
				"never"
			]
		}
	}
}
