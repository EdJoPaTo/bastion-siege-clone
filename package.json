		"test": "tsc --sourceMap && xo"
	},
	"type": "commonjs",
	"engines": {
		"node": ">=14"
	},
	"dependencies": {
		"@edjopato/telegraf-i18n": "^0.2.0",
		"array-filter-unique": "^2.1.0",
		"bastion-siege-logic": "^2.0.0",
		"locale-emoji": "^0.3.0",
		"random-item": "^3.0.0",
		"source-map-support": "^0.5.12",
		"telegraf": "^4.1.1",
		"telegraf-inline-menu": "^6.0.0",
		"telegraf-middleware-console-time": "^2.0.0",
		"telegraf-session-local": "^2.0.0",
		"telegraf-wikibase": "^0.10.1",
		"wikidata-entity-reader": "^2.0.0",
		"wikidata-person-names": "^1.0.0",
		"wikidata-sdk": "^7.4.3",
		"wikidata-sdk-got": "^2.0.1"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^1.0.1",
		"@types/node": "^15.0.1",
		"del-cli": "^3.0.1",
		"ts-node": "^9.0.0",
		"typescript": "^4.2.2",
		"xo": "^0.40.1"
	},
	"xo": {
		"semicolon": false,
		"rules": {
			"@typescript-eslint/no-non-null-assertion": "off",
			"@typescript-eslint/no-require-imports": "off",
			"@typescript-eslint/no-unsafe-assignment": "off",
			"@typescript-eslint/no-unsafe-call": "off",
			"@typescript-eslint/no-unsafe-member-access":"off",
			"@typescript-eslint/no-unsafe-return": "off",
			"unicorn/prefer-module": "off",
			"unicorn/prefer-node-protocol": "off"
		}
	}
}
